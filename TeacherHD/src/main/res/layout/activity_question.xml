        android:layout_width="@dimen/x164"
        android:layout_height="@dimen/x80"
        android:gravity="center"
        android:background="@drawable/question_preview_tv_icon"
        android:text="@string/question_analysis"
        android:textSize="@dimen/tv_normal"
        android:textColor="@color/white"
        app:layout_constraintStart_toEndOf="@id/verticalLine"
        app:layout_constraintTop_toBottomOf="@id/titleBar"
        android:layout_marginStart="@dimen/x20"
        />
    <View android:id="@+id/backgroundImg"
        android:layout_width="0dp"
        android:layout_height="@dimen/y54"
        android:background="@drawable/question_preview_bg_icon"
        app:layout_constraintStart_toEndOf="@id/verticalLine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/parseTv"
        android:layout_marginStart="@dimen/x20"
        android:layout_marginEnd="@dimen/x20"
        />
