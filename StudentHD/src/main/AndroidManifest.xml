<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yzy.ebag.student">

    <application
        android:name=".base.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.main.MainActivity"
            android:launchMode="singleTask"/>
        <activity android:name=".activity.WelcomeActivity"
            android:screenOrientation="landscape"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustUnspecified|stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.center.PersonalActivity"/>

        <activity android:name=".DemoActivity">

        </activity>
        <activity android:name=".TestActivity">

        </activity>
        <activity android:name=".QuestionTestActivity">

        </activity>
        <activity android:name=".activity.account.LoginActivity"/>

        <activity android:name=".activity.ToolsActivity"/>
        <activity android:name=".activity.book.BookListActivity"/>
        <activity android:name=".activity.SettingActivity"/>
        <activity android:name=".activity.homework.HomeworkActivity"/>
    </application>

</manifest>